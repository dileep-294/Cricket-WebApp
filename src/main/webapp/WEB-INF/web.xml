<?xml version="1.0" encoding="UTF-8"?>

<web-app  version="3.0"
          xmlns="http://java.sun.com/xml/ns/javaee"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" >

  <servlet>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>com.cricket.Servlets.UserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/users/*</url-pattern>
<!--    <url-pattern>/login</url-pattern>-->
  </servlet-mapping>

  <servlet>
    <servlet-name>TeamServlet</servlet-name>
    <servlet-class>com.cricket.Servlets.TeamServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TeamServlet</servlet-name>
    <url-pattern>/teams/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>GetTeamsById</servlet-name>
    <servlet-class>com.cricket.GetTeamsById</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetTeamsById</servlet-name>
    <url-pattern>/TeamsId</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.cricket.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Profile</servlet-name>
    <servlet-class>com.cricket.Profile</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Profile</servlet-name>
    <url-pattern>/Profile</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>InningsServlet</servlet-name>
    <servlet-class>com.cricket.Servlets.InningsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InningsServlet</servlet-name>
    <url-pattern>/innings</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>com.cricket.Servlets.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/LogoutServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>TournamentServlet</servlet-name>
    <servlet-class>com.cricket.TournamentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TournamentServlet</servlet-name>
    <url-pattern>/TournamentServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>PlayerServlet</servlet-name>
    <servlet-class>com.cricket.PlayerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PlayerServlet</servlet-name>
    <url-pattern>/PlayerServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Rating</servlet-name>
    <servlet-class>com.cricket.Rating</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Rating</servlet-name>
    <url-pattern>/Rating</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Schedule</servlet-name>
    <servlet-class>com.cricket.Servlets.ScheduleServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Schedule</servlet-name>
    <url-pattern>/schedule/*</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>TeamMembers</servlet-name>
    <servlet-class>com.cricket.TeamMembers</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TeamMembers</servlet-name>
    <url-pattern>/TeamMembers</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Toss</servlet-name>
    <servlet-class>com.cricket.Servlets.InningsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Toss</servlet-name>
    <url-pattern>/Toss</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Score</servlet-name>
    <servlet-class>com.cricket.Score</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Score</servlet-name>
    <url-pattern>/Score</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>


</web-app>